(slideId);\n  };\n\n  return (\n    <div className=\"imageSlide\">\n      <div className=\"slideList\">\n        {SLIDE_DATA.filter(({ id }) => id === currentSlideId).map(slide => (\n          <Slide key={slide.id} slideId={slide.id} imgUrl={slide.imageUrl} />\n        ))}\n      </div>\n      <ul className=\"slickDotWrapper\">\n        {SLIDE_DATA.map(({ id }) => (\n          <SlickDot\n            key={id}\n            currentSlideId={currentSlideId}\n            slideId={id}\n            checkSlideId={currentSlideId === id}\n            changeSlide={() => changeSlide(id)}\n          />\n        ))}\n      </ul>\n    </div>\n  );\n};\n\nexport default ImageSlide;\n","import React from 'react';\nimport '../NumBtn/NumBtn.scss';\n\nconst NumBtn = ({ minusOne, plusOne, numValue }) => {\n  return (\n    <div className=\"numBtn\">\n      <button className=\"numBtnMinus aNumBtn\" type=\"button\" onClick={minusOne}>\n        <i className=\"fas fa-minus\" />\n      </button>\n      <input className=\"numValue\" type=\"text\" value={numValue} readOnly />\n      <button className=\"numBtnPlus aNumBtn\" type=\"button\" onClick={plusOne}>\n        <i className=\"fas fa-plus\" />\n      </button>\n    </div>\n  );\n};\nexport default NumBtn;\n","import React, { useState } from 'react';\nimport { Link, useNavigate } from 'react-router-dom';\nimport NumBtn from '../ProductInfoBox/NumBtn/NumBtn';\nimport Button from '../Button/Button';\nimport { API_ADDRESS } from '../../apiConfig';\nimport { TRANSELATE } from '../../pages/ProductList/Transelate';\nimport '../ProductInfoBox/ProductInfoBox.scss';\n\nconst ProductInfoBox = ({\n  aptId,\n  name,\n  address,\n  type,\n  thumbnailImg,\n  location,\n  region,\n  open_date,\n  quantity,\n  area,\n  status,\n}) => {\n  const [numValue, setNumValue] = useState(1);\n  let token = localStorage.getItem('TOKEN');\n\n  const minusOne = () => {\n    numValue === 1 ? setNumValue(1) : setNumValue(numValue - 1);\n  };\n\n  const plusOne = () => {\n